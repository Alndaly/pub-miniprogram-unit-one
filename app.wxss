/**app.wxss**/
@import '/components/common/index.wxss';
@import "/styles/main.wxss";
@import "/styles/colorui.wxss";

@viewport {
  width: device-width;
  height: device-height;
  min-zoom: 1;
  max-zoom: 1;
  user-zoom: fixed;
}

